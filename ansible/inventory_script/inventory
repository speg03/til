#!/usr/bin/env python

import json


def main():
    inventory = {
        'target': {
            'hosts': ['host01']
        },
        '_meta': {
            'hostvars': {
                'host01': {
                    'var01': 'hello',
                    'python_interpreter': '/usr/bin/env python3'
                }
            }
        }
    }
    print(json.dumps(inventory))


if __name__ == '__main__':
    main()
